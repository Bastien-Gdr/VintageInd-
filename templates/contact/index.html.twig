{% extends 'base.html.twig' %}

{% block title %}Prendre contact !
{% endblock %}

{% block body %}
	<div class="card col-4 offset-4 mt-5 ">
		<div class="card-body col-10 offset-1">
			<h5 class="h2 text-center my-3 card-title">Pour prendre contact</h5>
			{{form_start(form)}}
			{{form_widget(form)}}

			<div class="text-center">
				<button type="submit" class="btn btn-lg btn-dark">Envoyer</button>
			</div>

			{{form_end(form)}}

		</div>
	</div>


{% endblock %}
